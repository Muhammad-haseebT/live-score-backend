# Copy this file to .env and fill values
# Do NOT commit .env

# Server
PORT=7860
SPRING_PROFILES_ACTIVE=local

# Database (local docker-compose)
DATABASE_NAME=livescore
DATABASE_USERNAME=postgres
DATABASE_PASSWORD=postgres
# If you want to connect to a remote DB, set DATABASE_URL explicitly:
# DATABASE_URL=jdbc:postgresql://host:5432/db

# Upload
UPLOAD_MODE=local

# ImageKit (only if UPLOAD_MODE=imagekit)
IMAGEKIT_PUBLIC_KEY=
IMAGEKIT_PRIVATE_KEY=
IMAGEKIT_URL_ENDPOINT=
